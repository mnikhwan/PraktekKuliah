void smile() {
  for(;;){

      
  display.clearDisplay();

  display.drawBitmap(
    (display.width()  - 128 ) / 2,
    (display.height() - 64) / 2,
    epd_bitmap_allArray[counter], 128, 64, 1);
  display.display();
    counter = (counter + 1) % 97; // increase the counter, but always go between 0-27
    digitalWrite(D6, HIGH);
  }
}


